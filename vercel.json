{
    "builds": [
        {
            "src": "app.ts",
            "use": "@now/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app.ts"
        }
    ],
    "git": {
        "deploymentEnabled": {
            "main": false,
            "dev": false,
            "api": false
        }
    }
}